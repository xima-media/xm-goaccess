<!-- Component Start: tag -->

<div class="tags">
    {% if title %}
        <h3 class="h5">{{ title }}</h3>
    {% endif %}
    <ul class="list list--inline">
        {% for item in items %}
            <li>
                <a href="#" class="button button--secondary">{{ item.text }}</a>
            </li>
        {% endfor %}
    </ul>
</div>

<!-- Components End: tag -->
