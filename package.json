{
  "name": "xm-dkfz-net",
  "version": "1.0.0",
  "description": "Asset build for DKFZ Intranet",
  "scripts": {
    "postinstall": "cd packages/xm_dkfz_net_prototype && npm install",
    "start": "sass --watch --load-path=node_modules packages/xm_dkfz_net_site/Resources/Private/Scss/app.scss packages/xm_dkfz_net_site/Resources/Public/Css/dist/app.css",
    "build": "cd packages/xm_dkfz_net_prototype && npm run build",
    "build:patternlab": "cd packages/xm_dkfz_net_prototype && npm run build:patternlab",
    "init": "cd packages/xm_dkfz_net_prototype && npm run init",
    "fix-styles": "cd packages/xm_dkfz_net_prototype && npm run fix-styles",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "cd packages/xm_dkfz_net_prototype && npm run serve"
  },
  "author": "Maik Schneider",
  "license": "proprietary",
  "dependencies": {
    "bootstrap": "^5.1.3",
    "sass": "^1.56.1"
  }
}
