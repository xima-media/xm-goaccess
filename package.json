{
  "name": "xm-dkfz-net",
  "version": "1.0.0",
  "description": "Asset build for DKFZ Intranet",
  "scripts": {
    "postinstall": "cd packages/xm_dkfz_net_prototype && npm install",
    "start": "sass --error-css --watch --load-path=node_modules packages/xm_dkfz_net_site/Resources/Private/Scss/app.scss packages/xm_dkfz_net_site/Resources/Public/Css/dist/app.css",
    "build": "sass --no-source-map --quiet --style=compressed --load-path=node_modules packages/xm_dkfz_net_site/Resources/Private/Scss/app.scss packages/xm_dkfz_net_site/Resources/Public/Css/dist/app.min.css",
    "build:patternlab": "cd packages/xm_dkfz_net_prototype && npm run build:patternlab",
    "init": "cd packages/xm_dkfz_net_prototype && npm run init",
    "fix-styles": "cd packages/xm_dkfz_net_prototype && npm run fix-styles",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "cd packages/xm_dkfz_net_prototype && npm run serve"
  },
  "author": "Maik Schneider",
  "license": "proprietary",
  "dependencies": {
    "autocompleter": "^7.0.1",
    "bootstrap": "^5.1.3",
    "sass": "^1.56.1",
    "swiper": "^8.4.5",
    "tippy.js": "^6.3.7",
    "tom-select": "^2.2.2",
    "typescript": "^4.9.3"
  }
}
