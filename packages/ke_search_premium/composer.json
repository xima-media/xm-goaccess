{
    "name": "tpwd/ke_search_premium",
    "type": "typo3-cms-extension",
    "description": "Additional functions for ke_search, the search extension for TYPO3.",
    "license": ["GPL-2.0+"],
    "keywords": ["TYPO3 CMS", "search", "ke_search", "kesearch", "faceting", "faceted search"],
    "require": {
        "typo3/cms-core": "^10.4 || ^11.5",
        "tpwd/ke_search": "^4.5.0"
    },
    "replace": {
        "teaminmedias-pluswerk/ke_search_premium": "self.version"
    },
	"autoload": {
        "psr-4": {
            "Tpwd\\KeSearchPremium\\": "Classes"
        },
        "classmap": ["Classes","Classes/Hooks","Classes/Lib"]
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "ke_search_premium"
        }
    }
}
