config {
  no_cache = 0
  sendCacheHeaders = 1
  debug = 0
  contentObjectExceptionHandler = 1
  compressJs = 0
  compressCss = 0
  concatenateJs = 0
  concatenateCss = 0
  admPanel = 1
  extTarget = _blank

  htmlTag.attributes.style = --scrollbar-width:17px;
  htmlTag.attributes.class = fx--header-not-sticky
}

[backend.user.isLoggedIn]
  config.no_cache = 1
[end]
