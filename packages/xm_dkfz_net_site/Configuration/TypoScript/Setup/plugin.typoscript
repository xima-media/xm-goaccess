plugin.tx_news {
  view {
    partialRootPaths.20 = EXT:xm_dkfz_net_site/Resources/Private/Extensions/General/Partials/
  }

  settings {
    defaultDetailPid = {$pidNewsDetail}
  }
}

plugin {

  tx_bwguild {
    view {
      partialRootPaths.20 = EXT:xm_dkfz_net_site/Resources/Private/Extensions/General/Partials/
    }
    settings {
      pidUserList = {$pidUserList}
      categoryConjunction = or
      categories = {$uidCategoryMarketplace}
      includeSubCategories = 1
    }
  }

  tx_bwguild_usersearch {
    settings {
      autocompleter {
        feGroup = fe_groups.title,fe_groups.dkfz_number
        function = tx_xmdkfznetsite_domain_model_contact.function
        committee = tx_xmdkfznetsite_domain_model_committee.name
      }
    }
  }
}

