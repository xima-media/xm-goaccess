[applicationContext matches "/^Development/"]
  config {
    debug = 1
    no_cache = 1
    compressCss = 0
    concatenateCss = 0
    compressJs = 0
    concatenateJs = 0
    contentObjectExceptionHandler = 0
  }

  page.meta.robots = noindex,nofollow

  page.includeCSS.app = EXT:xm_dkfz_net_site/Resources/Public/Css/dist/app.css
  page.includeJSFooter.app = EXT:xm_dkfz_net_site/Resources/Public/JavaScript/dist/app.js

  terminal.includeCSS.terminal = EXT:xm_dkfz_net_site/Resources/Public/Css/dist/terminal.css
  terminal.includeJSFooter.terminal = EXT:xm_dkfz_net_site/Resources/Public/JavaScript/dist/terminal.js
[global]
