[applicationContext matches "/^Development/"]
  config {
    debug = 1
    no_cache = 1
    compressCss = 0
    concatenateCss = 0
    compressJs = 0
    concatenateJs = 0
    contentObjectExceptionHandler = 0
  }

  page.meta.robots = noindex,nofollow

  page.includeCSS.prototype = EXT:xm_dkfz_net_prototype/Resources/Public/Css/app.css
  page.includeJS.prototype = EXT:xm_dkfz_net_prototype/Resources/Public/JavaScript/app.js
[global]
